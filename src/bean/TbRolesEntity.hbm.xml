<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bean.TbRolesEntity" table="tb_roles" schema="dingcan">
        <id name="id">
            <column name="Id" sql-type="bigint(20)"/>
        </id>
        <property name="roleName">
            <column name="RoleName" sql-type="longtext" not-null="true"/>
        </property>
        <property name="mealId">
            <column name="MealId" sql-type="bigint(20)"/>
        </property>
    </class>
</hibernate-mapping>